{
  "_version": "1.12.0",
  "sap.app": {
    "id": "demo.sot.sot",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "sourceTemplate": {
      "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
      "version": "1.40.12"
    },
    "dataSources": {
      "BackendDataSource": {
        "uri": "/backend/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      },
      "PeopleDetailDatasource": {
        "uri": "",
        "type": "JSON"
      }
    }
  },

  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },

  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "demo.sot.sot.view.Main",
      "type": "XML",
      "async": true,
      "id": "Main"
    },
    "dependencies": {
      "minUI5Version": "1.65.6",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "demo.sot.sot.i18n.i18n"
        }
      },
      "Backend": {
        "dataSource": "BackendDataSource",
        "settings": {
          "autoExpandSelect": true,
          "operationMode": "Server",
          "groupId": "$direct",
          "synchronizationMode": "None"
        }
      },
      "PeopleDetail": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "PeopleDetailDatasource"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "demo.sot.sot.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": "RouteMain",
          "target": ["TargetMain"]
        }
      ],
      "targets": {
        "TargetMain": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Main",
          "viewName": "Main"
        }
      }
    }
  }
}
